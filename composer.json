{
    "name": "yiistack/routing",
    "type": "library",
    "description": "Yii 3.0 annotated routing",
    "keywords": [
        "routing",
        "router",
        "annotation",
        "annotated"
    ],
    "homepage": "https://github.com/yiistack",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/yiistack/routing/issues?state=open",
        "source": "https://github.com/yiistack/routing"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4|^8.0",
        "yiisoft/aliases": "^1.0",
        "yiisoft/di": "^3.0@dev",
        "yiisoft/http": "^1.0@dev",
        "yiisoft/router": "^3.0@dev",
        "yiisoft/var-dumper": "^1.0",
        "yiistack/annotated": "^1.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        "roave/infection-static-analysis-plugin": "^1.5",
        "spatie/phpunit-watcher": "^1.23",
        "vimeo/psalm": "^4.2"
    },
    "provide": {
        "yiisoft/router-implementation": "1.0.0"
    },
    "autoload": {
        "psr-4": {
            "Yiistack\\Routing\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yiistack\\Routing\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "config-plugin": {
            "params": "config/params.php",
            "providers": "config/providers.php"
        }
    },
    "scripts": {
        "test": "phpunit --testdox --no-interaction",
        "test-watch": "phpunit-watcher watch"
    },
    "config": {
        "sort-packages": true
    }
}
