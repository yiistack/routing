{
    "name": "yiistack/routing",
    "type": "library",
    "description": "Yii 3.0 annotated routing",
    "keywords": [
        "routing",
        "router",
        "annotation",
        "annotated"
    ],
    "homepage": "https://github.com/yiistack",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/yiistack/routing/issues?state=open",
        "source": "https://github.com/yiistack/routing"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4|^8.0",
        "yiistack/annotated": "1.0.x-dev",
        "yiisoft/http": "1.0.x-dev",
        "yiisoft/router": "^3.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4",
        "phan/phan": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "Yiistack\\Routing\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yiistack\\Routing\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "scripts": {
        "phan": "phan --progress-bar -o analysis.txt"
    },
    "config": {
        "sort-packages": true
    }
}
